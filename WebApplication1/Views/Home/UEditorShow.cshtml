@using WebApplication1.Models;
@{
    ViewBag.Title = "UEditorShow";
    List<ueditor> ueditors = ViewData["ueditor"] as List<ueditor>;
}

<h2>UEditorShow</h2>

<ol>
    @{ 
        for(int i = 0;i < ueditors.Count; i++)
        {
            <li>
                @Html.Raw(Server.UrlDecode(ueditors[i].content))
            </li>
        }
    }
</ol>

<div>
    <a href="/Home/uEditor">返回新建</a>
</div>